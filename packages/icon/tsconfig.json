{
  "extends": "@tiki/tsconfig/base.json",
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "rootDir": "./",
    "baseUrl": "./src",
    "outDir": "./dist",
    "composite": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "jsx": "react-jsx",
  },
  "include": ["src", "**/*.ts", "**/*.tsx", "svgr.d.ts"],
  "exclude": ["node_modules", "dist"],
}
  